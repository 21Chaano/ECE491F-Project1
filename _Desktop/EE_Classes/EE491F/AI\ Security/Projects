      "source": [
        "**Arithmetic operations**\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 11,
      "metadata": {
        "id": "oCBHlzlbeZ4x",
        "outputId": "b444a474-d059-4c23-a749-2b98d94de081",
        "colab": {
